<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Estilos/Shared/HeaderPrincipal.css")
    @Styles.Render("~/Estilos/HomeLogin/Login.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Scripts/HomeLogin/Login.js")
    @Styles.Render("~/Content/css")
</head>
<body>

    <header>
        <button type="button" class="header-logo" onclick="ir_a('/HomePrincipal/Index')">
            <img class="navbar-brand" src="~/Imagenes/logo.png" alt="FloristeriaMillan.png" />
        </button>
    </header>

    <main>
        @RenderBody()
    </main>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>