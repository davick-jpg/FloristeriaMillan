                <span class="material-icons-outlined">
                    arrow_back_ios
                </span>
            </button>
            @if (Session["Productos_recomendados"] != null)
            {
                FloristeriaMillan.Models.Producto[] productos = (FloristeriaMillan.Models.Producto[])Session["Productos_recomendados"];
                for (int i = 0; i < productos.Length; i++)
                {
                    <div class="contenedor-tarjeta">
