            <span class="material-icons-outlined">
                arrow_back_ios
            </span>
        </button>
        @if (Session["Banners"] != null)
        {
            String[] direcciones = (String[])Session["Banners"];
            foreach (String direccion in direcciones)
            {
                <div class="contenedor_promocion">
                    <img src=@direccion.ToString() alt="Alternate Text" loop="false" autoplay />
                </div>
            }
        }
        <button type="button" class="boton-navegacion-productos-recomnedados" onclick="avanzar_banner()" id="boton-avanzar-banner-promociones">
            <span class="material-icons-outlined">
                arrow_forward_ios
            </span>
        </button>
    </div>
</div>
