                <span class="material-icons-outlined">
                    arrow_forward_ios
                </span>
            </button>
        </div>
    </div>
</div>
<i class="divisor"></i>
<div class="contenedor-banner-promociones">
    <div class="carrusel-banner-promociones" id="carrusel-banner-promociones">
